# Deployment private key
PRIVATE_KEY=

# Admin address (optional, will use private key address if not set)
ADMIN_ADDRESS=

# RPC URLs
AVALANCHE_RPC_URL=
BASE_RPC_URL=
OPTIMISM_RPC_URL=
ARBITRUM_RPC_URL=
POLYGON_RPC_URL=
ETHEREUM_RPC_URL=

# Flags Boolean default to true
SETUP_PERMISSIONS=true

# Etherscan API Keys for verification
ETHERSCAN_API_KEY=
BASESCAN_API_KEY=
ARBISCAN_API_KEY=
OPTIMISTIC_API_KEY=
POLYGONSCAN_API_KEY=

# Comma-separated list of target chain IDs for LZ configuration
# Arb: 42161, Avalanche: 43114, Base: 8453, BNB: 56, 
# Ethereum: 1, Fantom: 250, Linea: 59144, Optimism: 10, 
# Polygon: 137
TARGET_CHAIN_IDS=

# Arb: 30110, Avalanche: 30106, Base: 30184, BNB: 30102,
# Ethereum: 30101, Fantom: 30112, Linea: 30183, Optimism: 30111,
# Polygon: 30109
TARGET_ENDPOINT_IDS=

# Destination chain ID
# Arb: 42161, Avalanche: 43114, Base: 8453, BNB: 56, 
# Ethereum: 1, Fantom: 250, Linea: 59144, Optimism: 10, 
# Polygon: 137
DST_CHAIN_ID=

# LayerZero configuration
LZ_GAS_LIMIT=500000
LZ_NATIVE_VALUE=200000

# Rewards delegate address
REWARDS_DELEGATE=

# Vault addresses (comma-separated)
VAULT_ADDRESSES=
